<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <!-- Routes -->
    <record id="fsm_route_form" model="ir.ui.view">
        <field name="name">fsm.route.form</field>
        <field name="model">fsm.route</field>
        <field name="inherit_id" ref="fieldservice.fsm_route_form"/>
        <field name="arch" type="xml">
            <field name="person_id" position="after">
                <field name="vehicle_id"
                       groups="fieldservice_vehicle.group_fsm_vehicle"/>
            </field>
        </field>
    </record>

</odoo>
