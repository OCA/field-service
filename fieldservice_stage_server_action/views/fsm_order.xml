<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Field Service Order Form Extended-->
    <record id="fsm_order_form" model="ir.ui.view">
        <field name="name">fsm.order.form</field>
        <field name="model">fsm.order</field>
        <field name="inherit_id" ref="fieldservice.fsm_order_form"/>
        <field name="arch" type="xml">
            <field name="scheduled_date_end" position="after">
                <field name="next_request_assign"/>
                <field name="has_stage_changed" invisible="1"/>
            </field>
        </field>
    </record>

</odoo>
