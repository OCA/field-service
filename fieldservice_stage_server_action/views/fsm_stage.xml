<odoo>

    <!-- Fieldservice Stage Form View-->
    <record id="fsm_server_action_stage_form_view" model="ir.ui.view">
        <field name="name">fsm.server.action.stage.form</field>
        <field name="model">fsm.stage</field>
        <field name="inherit_id" ref="fieldservice.fsm_stage_form_view"/>
        <field name="arch" type="xml">
            <field name="sequence" position="after">
                <field name="action_id"/>
            </field>
        </field>
    </record>
</odoo>
